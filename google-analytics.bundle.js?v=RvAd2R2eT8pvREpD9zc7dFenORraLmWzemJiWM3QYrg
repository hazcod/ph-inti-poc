(()=>{var e=function(){var e=$("#Inject-cookie-consent-name").val(),o=$("#Inject-google-analytics-measurement-id").val(),n=($("#Inject-web-domain").val().replace("http://","").replace("https://",""),function(e){var o=document.cookie.match(new RegExp("(^| )"+e+"=([^;]+)"));if(o)return o[2]}(e));if(n){if("closed"!==n)return window["ga-disable-"+o]=!0,void console.log("Cannot initialise Google Analytics: consent cookie isn't accepted!");if(window.gtag)console.log("Cannot initialise Google Analytics: GA is already initialized!");else if(o){var i=document.createElement("script");i.type="text/javascript",i.async=!0,i.src="https://www.googletagmanager.com/gtag/js?id="+o,document.head.prepend(i),window.dataLayer=window.dataLayer||[],window.gtag=function(){window.dataLayer.push(arguments)},window.gtag("js",new Date),window.gtag("config",o,{page_location:window.location.href.split("?")[0]})}else console.log("Cannot initialise Google Analytics: no ga_measurement_id set.")}else console.log("Cannot initialise Google Analytics: no cookie consent cookie!")};$(document).ready((function(){$("#cookie-consent-close").click((function(){setTimeout(e)})),e()}))})();